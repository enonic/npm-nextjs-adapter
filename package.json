{
  "name": "@enonic/nextjs-adapter",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "author": "Pavel Milkevich <pmi@enonic.com>",
  "contributors": [
    "Christian Westgaard <cwe@enonic.com>"
  ],
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./views/*": {
      "types": "./dist/views/*.d.ts",
      "import": "./dist/views/*.jsx",
      "require": "./dist/views/*.jsx"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "keywords": [
    "enonic",
    "nextjs",
    "adapter",
    "typescript",
    "ecmascript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "del-cli dist/ enonic-nextjs-adapter-*.tgz",
    "check:types": "tsc --noEmit",
    "madge": "madge -i docs/madge.png -x '(constants|env)\\.ts' src/index.ts",
    "lint": "eslint --quiet --cache src",
    "lint:fix": "eslint --fix src",
    "prebuild": "npm run clean",
    "prepack": "concurrently -c auto -g --timings npm:test npm:lint npm:build",
    "premadge": "mkdirp docs",
    "prepublishOnly": "node setversion.js",
    "pretest": "del-cli coverage",
    "test": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@enonic/react-components": "^6.0.0",
    "@formatjs/intl-localematcher": "^0.6.1",
    "gqlmin": "^0.3.1",
    "negotiator": "^1.0.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@edge-runtime/jest-environment": "^4.0.0",
    "@enonic-types/core": "^7.15.4",
    "@enonic/eslint-config": "^2.1.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.0",
    "babel-jest": "^30.1.2",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.1.2",
    "madge": "^8.0.0",
    "mkdirp": "^3.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "types": "index.d.ts",
  "peerDependencies": {
    "html-react-parser": "^5.2.6",
    "next": "^14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
